{
  "manifest_version": 2,

  "name": "Qrcode maker",
  "description": "This extension make a qrcode image for a url .",
  "version": "1.0",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "qrcode-maker.html"
  },
  "permissions": [
    "tabs"
  ]
}
